pkgname='moos-run'
pkgver=20250808
pkgrel=1
pkgdesc='Scripts for executing single-file programs.'
arch=('any')
options=('!debug')
depends=(
)
source=(
    'run_c'
    'run_cpp'
)
sha256sums=(
    '19924decdb0e1af6f8f74b350d7b08dede6717b80e6e2a572a458ebde8b3decc'
    'e15f238fc7530f9799eadacd943315fb7e54402c4ca5b044faa2c54fe5ae7db3'
)

package() {
    install -Dm755 "run_c" "$pkgdir/usr/bin/run_c"
    install -Dm755 "run_cpp" "$pkgdir/usr/bin/run_cpp"
}
