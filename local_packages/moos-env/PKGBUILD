pkgname='moos-env'
pkgver=20260108
pkgrel=1
pkgdesc='Dot files that configure the shell.'
arch=('any')
options=('!debug')
source=(
    'rc_moos.sh'
    '.bashrc'
    '.bash_profile'
)
sha256sums=(
    '97359b569a29c3f6aab278087f058fc6950d4f0e336c81e54f41eecf7a6304e6'
    '942f6075cffa731f0a9017892a5795c16e539a12c8844f03d6b14f6286d0e9d3'
    '2e0ddc76f74a56fafdeba6bdb65589a5735586b9b25cbbe194e559352aecfe91'
)

pkgver() {
    date +%Y%m%d
}

package() {
    _skel="$pkgdir/etc/moos-skel"
    install -Dm644 "rc_moos.sh" "$_skel/.bashrc.d/moos.sh"
    install -Dm644 ".bashrc" "$_skel/.bashrc"
    install -Dm644 ".bash_profile" "$_skel/.bash_profile"
}
